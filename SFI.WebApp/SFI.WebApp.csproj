<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <AssemblyName>SemanticFileInspector.WebApp</AssemblyName>
    <LangVersion>9.0</LangVersion>
    <Nullable>enable</Nullable>
    <RootNamespace>IS4.SFI.WebApp</RootNamespace>
    <Authors>IS4</Authors>
    <DocumentationFile>bin\SemanticFileInspector.WebApp.xml</DocumentationFile>
    <AssemblyTitle>Semantic File Inspector Web Application</AssemblyTitle>
